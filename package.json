{
  "name": "castable",
  "version": "0.1.0",
  "description": "making safari castable",
  "main": "castable Extension/Resources/script.js",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "build:script": "webpack --help",
    "check": "npm run lint && npm run build && npm run test",
    "clean": "rm -rf dist",
    "dev": "webpack --watch",
    "lint": "eslint . --cache --ext .ts && eslint 'castable Extension/ts-test' --ext .ts",
    "prepublishOnly": "npm run check",
    "test": "mocha -r ts-node/register 'castable Extension/ts-test/**/*-test.ts'"
  },
  "keywords": [
    "chromecast"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhleong/castable.git"
  },
  "author": "Daniel Leong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhleong/castable/issues"
  },
  "homepage": "https://github.com/dhleong/castable#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.4",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@types/safari-extension": "0.0.28",
    "debug": "^4.2.0"
  }
}
